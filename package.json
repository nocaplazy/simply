{
  "name": "simply",
  "packageManager": "yarn@3.4.1",
  "version": "1.1",
  "description": "Easily check your school with Chrome extension",
  "scripts": {
    "dev": "env-cmd webpack serve --mode development --config ./config/webpack.config.js",
    "build": "env-cmd webpack --mode production --progress --config ./config/webpack.config.js",
    "prepare": "husky install",
    "lint": "npx eslint --ext .tsx --ext .ts .",
    "lint:fix": "npx eslint --ext .tsx --ext .ts . --fix",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/runtime-corejs3": "^7.18.9",
    "@types/node": "^18.14.2",
    "@types/react": "^18.0.10",
    "@types/react-dom": "^18.0.5",
    "@types/react-outside-click-handler": "^1.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "airbnb": "^0.0.2",
    "babel-loader": "^8.2.5",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": ">=6",
    "image-webpack-loader": "^8.1.0",
    "lint-staged": ">=10",
    "mini-css-extract-plugin": "^2.6.0",
    "pinst": ">=2",
    "prettier": "^2.6.2",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.9.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.0.10",
    "axios": "^0.27.2",
    "core-js": "^3.26.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-outside-click-handler": "^1.3.0",
    "react-router-dom": "^6.7.0",
    "react-toastify": "^9.0.7",
    "recoil": "^0.7.4",
    "styled-components": "^5.3.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
